<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.relaxed.common.risk.repository.mapper.FieldMapper">
  <resultMap id="fieldMap" type="com.relaxed.common.risk.model.entity.Field">
                  <id property="id" column="ID"/>
                        <result property="modelId" column="MODEL_ID"/>
                        <result property="fieldName" column="FIELD_NAME"/>
                        <result property="label" column="LABEL"/>
                        <result property="fieldType" column="FIELD_TYPE"/>
                        <result property="validateType" column="VALIDATE_TYPE"/>
                        <result property="validateArgs" column="VALIDATE_ARGS"/>
                        <result property="indexed" column="INDEXED"/>
                        <result property="createTime" column="CREATE_TIME"/>
                        <result property="updateTime" column="UPDATE_TIME"/>
            </resultMap>
 
	<sql id="Base_Column_List">
		ID,        
		MODEL_ID,        
		FIELD_NAME,        
		LABEL,        
		FIELD_TYPE,        
		VALIDATE_TYPE,        
		VALIDATE_ARGS,        
		INDEXED,        
		CREATE_TIME,        
		UPDATE_TIME        
	</sql>

	<sql id="Base_Alias_Column_List">
		f.ID,        
		f.MODEL_ID,        
		f.FIELD_NAME,        
		f.LABEL,        
		f.FIELD_TYPE,        
		f.VALIDATE_TYPE,        
		f.VALIDATE_ARGS,        
		f.INDEXED,        
		f.CREATE_TIME,        
		f.UPDATE_TIME        
	</sql>
</mapper>