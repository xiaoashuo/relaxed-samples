<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.relaxed.common.risk.repository.mapper.PreItemMapper">
	<resultMap id="preItemMap" type="com.relaxed.common.risk.model.entity.PreItem">
		<id property="id" column="ID"/>
		<result property="modelId" column="MODEL_ID"/>
		<result property="sourceField" column="SOURCE_FIELD"/>
		<result property="sourceLabel" column="SOURCE_LABEL"/>
		<result property="destField" column="DEST_FIELD"/>
