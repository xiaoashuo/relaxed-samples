	<sql id="Base_Alias_Column_List">
		pi.ID,
		pi.MODEL_ID,
		pi.SOURCE_FIELD,
		pi.SOURCE_LABEL,
		pi.DEST_FIELD,
pi.DEST_FIELD_TYPE,
		pi.LABEL,
		pi.ARGS,
		pi.PLUGIN,
		pi.CONFIG_JSON,
		pi.REQ_TYPE,
		pi.STATUS,        
